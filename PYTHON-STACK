class Stack:
    def __init__(self):
        self.stack=[7]


    def add(self,data):
        if data not in self.stack:
            self.stack.append(data)
            return True
        else:
            return  "Sorry Your stack is owerflow"

    def peek(self):
        return self.stack[-1]

object1 = Stack()
object1.add("Monday")
object1.add("Tuesday")
object1.add("Wednesday")
object1.add("Thursday")
object1.add("Friday")
object1.add("Saturday")
object1.add("Sunday")
print(object1.peek())

print(object1.add("Monday"))
